<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE app [
 <!ENTITY name "KnownGenesToBed">
 <!ENTITY package "com.github.lindenb.jvarkit.tools.misc">
]>
<app xmlns="http://github.com/lindenb/jvarkit/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:svg="http://www.w3.org/2000/svg" app="&name;" package="&package;" __placeholder__="1">
<description>KnownGene to BED</description>


<options>
	<option name="hide_introns" type="boolean" opt="i" longopt="introns"  >
		<description>don't print introns</description>
	</option>
	<option name="hide_utrs" type="boolean" opt="u" longopt="utrs" >
		<description>don't print utrs</description>
	</option>
	<option name="hide_cds" type="boolean" opt="c" longopt="cds" >
		<description>don't print cds</description>
	</option>
	<option name="hide_exons" type="boolean" opt="x" longopt="exons" >
		<description>don't print exons</description>
	</option>
	<option name="hide_transcripts" type="boolean" opt="x" longopt="transcripts" >
		<description>don't print transcripts</description>
	</option>
</options>



<!-- __PLACEHOLDER__ --></app>
