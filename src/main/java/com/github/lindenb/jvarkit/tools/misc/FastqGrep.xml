<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE app [
 <!ENTITY name "FastqGrep">
 <!ENTITY package "com.github.lindenb.jvarkit.tools.misc">
]>
<app xmlns="http://github.com/lindenb/jvarkit/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:svg="http://www.w3.org/2000/svg" app="&name;" package="&package;" __placeholder__="1">
<description>Grep reads names in fastq</description>



<options>
	<option name="n_before_remove" type="int" opt="n" longopt="n" default="-1">
		<description>when found, remove the read from the list of names when found more that 'n' time (increase speed); -1 = use all</description>
	</option>
	<option name="readNameFile" type="input-file" opt="f" longopt="readFile" >
		<description>file containing a list of read names</description>
	</option>
	<option name="readNameStrings" type="string-set" opt="R" longopt="name" >
		<description>add the read.</description>
	</option>
	<option name="inverse" type="boolean" opt="V" longopt="inverse" default="false">
		<description>file containing a list of read names</description>
	</option>
</options>



<!-- __PLACEHOLDER__ -->


</app>
