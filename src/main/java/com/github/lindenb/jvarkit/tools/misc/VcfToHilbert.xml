<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE app [
 <!ENTITY name "VcfToHilbert">
 <!ENTITY package "com.github.lindenb.jvarkit.tools.misc">
]>
<app xmlns="http://github.com/lindenb/jvarkit/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:svg="http://www.w3.org/2000/svg" app="&name;" package="&package;" __placeholder__="1">
<description>Plot a Hilbert Curve from a VCF file</description>
<input type="vcf"/>
	

<options>
	<option name="reference" type="input-file" opt="R" longopt="REF">
		<description id="faidx"/>
	</option>
	<option name="imageWidth" type="int" opt="w" longopt="width" default="1000">
		<description> with/height of the final picture</description>
	</option>
	<option name="radiusSize" type="float" opt="r" longopt="radius" default="3.0f">
		<description> radius of a point</description>
	</option>
</options>


<!-- __PLACEHOLDER__ -->


</app>
