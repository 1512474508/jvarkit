<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE app [
 <!ENTITY name "VcfFilterSequenceOntology">
 <!ENTITY package "com.github.lindenb.jvarkit.tools.misc">
]>
<app xmlns="http://github.com/lindenb/jvarkit/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:svg="http://www.w3.org/2000/svg" app="&name;" package="&package;" __placeholder__="1">
<description>Filter a VCF file annotated with SNPEff or VEP with terms from Sequence-Ontology. Reasoning : Children of user's SO-terms will be also used.</description>
<input type="vcf"/>
<output type="vcf"/>
<snippet id="read-string-set"/>

	


<options>
	<option name="inverse_result" type="boolean" opt="i" longopt="inverse" default="false" >
		<description> Combined Annotation Dependent Depletion (CADD) Tabix file URI .</description>
	</option>
	<option name="just_list" type="boolean" opt="S" longopt="list" default="false" >
		<description>list the available SO accession and exit.</description>
	</option>
	<option name="disable_reasoning" type="boolean" opt="d" longopt="list" default="false" >
		<description>disable reasoning, don't use term's children.</description>
	</option>
	<option name="userTermsAsString" type="string-set" opt="A" longopt="term"  >
		<description>p(SO:ACN). add this SO:ACN</description>
	</option>
	<option name="termFile" type="input-file" opt="f" longopt="term"  >
		<description>file of SO accession numbers</description>
	</option>
</options>

<!-- __PLACEHOLDER__ -->
</app>
