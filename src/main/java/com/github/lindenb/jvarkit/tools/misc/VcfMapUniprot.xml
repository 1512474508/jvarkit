<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE app [
 <!ENTITY name "VcfMapUniprot">
 <!ENTITY package "com.github.lindenb.jvarkit.tools.misc">
]>
<app xmlns="http://github.com/lindenb/jvarkit/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:svg="http://www.w3.org/2000/svg" app="&name;" package="&package;" __placeholder__="1">
<description>Map uniprot features on VCF annoted with VEP or SNPEff</description>
<input type="vcf"/>
<output type="vcf"/>

<options>
	<option name="taxonid" type="int" opt="t" longopt="taxon"   default="9606">
		<description>NCBI taxon id</description>
	</option>
	<option name="intervalMillisec" type="long" opt="w" longopt="millisec"   default="1000">
		<description>wait between two uniprot queries</description>
	</option>
</options>

<arguments> </arguments><!-- __PLACEHOLDER__ --></app>
