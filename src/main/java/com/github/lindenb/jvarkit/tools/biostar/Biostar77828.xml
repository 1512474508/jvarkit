<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE app [
 <!ENTITY name "Biostar77828">
 <!ENTITY package "com.github.lindenb.jvarkit.tools.biostar">
]>
<app xmlns="http://github.com/lindenb/jvarkit/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:svg="http://www.w3.org/2000/svg" app="&name;" package="&package;" __placeholder__="1">
<description>Divide the human genome among X cores, taking into account gaps See http://www.biostars.org/p/77828/ </description>
<input type="stdin-or-one"/>

<options>
	<option  name="MINC" opt="minc" longopt="minc" type="int" default="20">
		<description>min core</description>
	</option>
	<option  name="MAXC" opt="maxc" longopt="maxc" type="int" default="30">
		<description>max core</description>
	</option>
	<option  name="N_ITERATIONS" opt="iter" longopt="iter" type="long" default="1000000">
		<description>number of iterations</description>
	</option>
</options>



<arguments> </arguments><!-- __PLACEHOLDER__ -->

</app>
