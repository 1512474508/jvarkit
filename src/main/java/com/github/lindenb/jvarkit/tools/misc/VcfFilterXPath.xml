<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE app [
 <!ENTITY name "VcfFilterXPath">
 <!ENTITY package "com.github.lindenb.jvarkit.tools.misc">
]>
<app xmlns="http://github.com/lindenb/jvarkit/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:svg="http://www.w3.org/2000/svg" app="&name;" package="&package;" __placeholder__="1">
<description>Filter a VCF with a XPATH expression on a INFO tag containing a base64 encodede xml document</description>
<input type="vcf"/>
<output type="vcf"/>
					

<options>
	<option name="infoTag" type="string" opt="T" longopt="tag"  >
		<description> INFO tag containing a base64-encoded XML document</description>
	</option>
	<option name="xpathExpression" type="string" opt="x" longopt="xpath"  >
		<description> XPath expression</description>
	</option>
	<option name="namespaceStrList" type="string-list" opt="n" longopt="namespace"  >
		<description>prefix=uri (add this namespace mapping to xpath context) </description>
	</option>
</options>



<!-- __PLACEHOLDER__ --></app>
