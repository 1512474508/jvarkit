<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE app [
 <!ENTITY name "BlastMapAnnotations">
 <!ENTITY package "com.github.lindenb.jvarkit.tools.blastmapannots">
]>
<app xmlns="http://github.com/lindenb/jvarkit/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:svg="http://www.w3.org/2000/svg" app="&name;" package="&package;" __placeholder__="1">
<description>XML sequence file Genbank.xml or uniprot.xml.</description>
<input type="stdin-or-one"/>

<options>
	<option name="APPEND_ACN" type="boolean" opt="APC" longopt="APC" default="false" >
		<description>append the sequence accession before the feature name</description>
	</option>
	<option name="INCL" type="string-set" opt="INCLUDE" longopt="INCLUDE" >
		<description>Restrict to uniprot/feature/type of genbank/feature/key</description>
	</option>
	<option name="EXCL" type="string-set" opt="EXCLUDE" longopt="EXCLUDE" >
		<description>Exclude uniprot/feature/type of genbank/feature/key.</description>
	</option>
	<option name="IN" type="input-file" opt="IN" longopt="IN" >
		<description>XML sequence file Genbank.xml or uniprot.xml. Required. </description>
	</option>
</options>


<arguments> </arguments><!-- __PLACEHOLDER__ --></app>
